    <script>
        const onRemove = id => {
            swal({
                title: "Are you sure?",
                text: "Once deleted, you will not be able to recover this imaginary file!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        const url = `/san-pham/remove/${id}`;
                        fetch(url, {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application.json'
                            }
                        }).then(res => res.json())
                            .then(data => {
                                swal("deleted", {
                                    icon: "success",
                                });
                                window.location.reload();
                            });
                    }
                });
        }

        //lấy queryString từ url
        const queryString = window.location.search;
        //console.log(queryString);
        // ?page=...&size=...
        const urlParams = new URLSearchParams(queryString);
        const page = urlParams.get('page');
        console.log(page);

        //set số  trang hiện tại vào h4
        //let numb = document.getElementById("pageNumber").innerHTML;
        document.getElementById("pageNumber").innerHTML = page;


        function changePage(stat) {
            // Start position 

            if (stat == true) {
                //console.log(page);
                const url = `/nguoi-dung?page=${parseInt(page) + 1}`;
                window.location.replace(url);
            } else if (stat == false) {
                if (parseInt(page) != 1) {
                    const url = `/nguoi-dung?page=${parseInt(page) - 1}`;
                    window.location.replace(url);
                }

            }
        }

    </script>